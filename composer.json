{
    "name": "josecelano/php-object-literal",
    "description": "Factory class for creating PHP object literals",
    "keywords": ["object literal"],
    "homepage": "https://github.com/josecelano/php-object-literal",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Celano",
            "email": "josecelano@gmail.com",
            "homepage": "http://josecelano.com"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "ObjectLiteral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ObjectLiteral\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ],
        "test-ci": [
            "vendor/bin/phpunit --coverage-text --coverage-clover=build/unit_coverage.xml"
        ]
    }
}